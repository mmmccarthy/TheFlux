include includes/mixins
html(lang="en")
	head
		meta(charset='utf-8')
		meta(http-equiv="X-UA-Compatible", content="IE=edge")
		meta(name='viewport', content='width=device-width, initial-scale=1.0')
		meta(name='keywords', content='Transition, Journalism, Northwestern, Medill')
		meta(name='apple-mobile-web-app-capable', content='yes')
		meta(name='apple-touch-fullscreen', content='yes')

		+meta("The Flux", "Stories On Transition", "http://graphics8.nytimes.com/images/2012/02/19/us/19whitney-span/19whitney-span-articleLarge.jpg", "@MedillSchool", "@zlwise", "the-flux.com")
		
		title The Flux

		// CSS
		link(rel='stylesheet', href='css/theflux.css?v10')

		// HTML5 shim, for IE6-8 support of HTML elements
		//if lt IE 9
			script(src='https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js')
			script(src='https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js')
		
		// Icon
		link(rel='shortcut icon', href='img/ico/favicon.png')
		link(rel='apple-touch-icon-precomposed', href='img/ico/favicon.png')
		link(rel='apple-touch-icon-precomposed', href='img/ico/apple-touch-icon-144.png', sizes="144x144")

	body
		
		// NAVBAR
		//include includes/navbar
		+navbar(false)

		// STORY COVER
		section#story-cover.story-cover
			div.story-cover-image(style="background-image: url('img/example.jpg');")
			div.story-cover-arrow
			div.story-cover-content
				h1 Awesome Story Title Goes Here
				p.lead Deck or lead/lede will go here. It can be a sentence or two but shouldn't go over that
				p.byline by John Doe
				p.byline Photographs by Jane Doe

		section.story-cover-caption
			div.caption
				| Senior Jacqueline Martinez and her boyfriend share a bus ride on her second to last day of high school. The two are expecting their first child this November.
		article.lazyload

